	</bean>
       

	<bean id="genericRestServlet" class="com.uhg.restproxy.services.RestProxyServlet">

              <property name="requestHandler" ref="genericRestRequestHandler" />

       </bean>

 

       <bean id="genericRestResponseHandler" class="com.uhg.restproxy.proxy.impl.ResponseHandlerImpl">

              <property name="responseNameGenerator" ref="genericResponseNameGenerator" />

              <property name="dceMessagePath" value="#{dceRestMessagePath}" />

       </bean>

 

       <bean id="genericResponseNameGenerator"

              class="com.uhg.restproxy.proxy.impl.responsenamegenerator.dce.DefaultResponseNameGenerator" />

 

 

       <bean id="genericRestRequestHandler" class="com.uhg.restproxy.proxy.impl.RestProxyImpl">

              <property name="restBaseUrl" value="#{dceRestEndpointPrefix}" />

              <property name="responseHandler" ref="genericRestResponseHandler" />

              <property name="soapProxyPassThroughOnSwitch" value="#{soapProxyPassThroughOnSwitch}" />

       </bean>
             
             
             
