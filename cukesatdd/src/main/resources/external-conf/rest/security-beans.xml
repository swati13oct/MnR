<!-- create payment Security Interceptors -->

<bean id="wesbCreatePaymentSecurityInterceptor" class="org.springframework.ws.soap.security.wss4j.Wss4jSecurityInterceptor">
        <property name="securementActions" value="Signature"/>
        <property name="securementMustUnderstand" ref="wesbSecurementMustUnderstand"/>
        <property name="securementUsername" ref="wssKeystoreUsername_CreatePayment"/>
        <property name="securementPassword" ref="wssKeystorePassword_CreatePayment"/>
        <property name="securementSignatureParts" ref="wesbSecurementSignatureParts" />        
        <property name="securementSignatureCrypto">
            <bean class="org.springframework.ws.soap.security.wss4j.support.CryptoFactoryBean">
                <property name="keyStorePassword" ref="wssKeystorePassword_CreatePayment"/>
                <property name="keyStoreLocation" ref="wssKeystoreLocation_CreatePayment"/>
            </bean>
        </property>
    </bean>

 <!-- UPM3 Security Interceptors -->
    <bean id="upmUsernameTokenSecurityInterceptor" class="org.springframework.ws.soap.security.wss4j.Wss4jSecurityInterceptor">
        <property name="securementActions" value="Timestamp UsernameToken"/>
        <property name="securementUsername" value="umr_system_id"/>
        <property name="securementPassword" value="BRrxR%&lt;a"/> <!-- BRrxR%<a -->
        <property name="securementPasswordType" ref="wesbPasswordType"/>
        <property name="securementUsernameTokenElements" value="Nonce Created"/>
    </bean>
	
