<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:util="http://www.springframework.org/schema/util"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/aop
           http://www.springframework.org/schema/aop/spring-aop-3.0.xsd		   
           http://www.springframework.org/schema/util 
           http://www.springframework.org/schema/util/spring-util-3.1.xsd">

	<!-- GetDocumentMaterials Service -->
    <bean id="getDocumentMaterialsServiceImpl" class="com.uhg.uhcmr.service.getdocumentmaterials.GetDocumentMaterialsServiceImpl">
        <property name="getDocumentMaterialsServiceDao" ref="getDocumentMaterialsServiceDaoDecorator" />
    </bean>

           
    <bean id="getDocumentMaterialsServiceDaoDecorator" class="com.uhg.uhcmr.service.getdocumentmaterials.impl.dao.GetDocumentMaterialsServiceDaoDecorator">
        <property name="dao" ref="getDocumentMaterialsServiceDao"/>
		<property name="pdpClaimsAddressMap" ref="pdpClaimsAddressMap"/>
    </bean>

	<util:map id="pdpClaimsAddressMap">
        <entry key="2014" value-ref="pdpClaimsAddresses2014"/>
    </util:map>        
    
    <util:map id="pdpClaimsAddresses2014">
        <entry key="UnitedHealthcare MedicareRx for Groups (PDP)" value-ref="plan1Address2014"/>
    </util:map>
    
    <bean id="plan1Address2014" class="com.uhg.uhcmr.ws.common.domain.Address">
        <property name="addressLine1" value="P.O. Box 29046" />
        <property name="addressLine2" value="Hot Springs, AR 71903" />
        <property name="city" value="" />
        <property name="stateCode" value="" />
        <property name="zipCode" value="" />        
    </bean>    

</beans>